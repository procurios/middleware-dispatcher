{
  "name": "procurios/middleware-dispatcher",
  "description": "Simple PSR-15 compliant middleware dispatcher.",
  "license": "MIT",
  "keywords": [
    "middleware",
    "dispatcher",
    "http",
    "psr-15",
    "psr-7"
  ],
  "require": {
    "php": ">=7.0",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Procurios\\Http\\MiddlewareDispatcher\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Procurios\\Http\\MiddlewareDispatcher\\test\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
